#Bash script to run using Azure CLI
az keyvault secret set --vault-name your-key-vault-name --name db-hostname --value "<ModusETP_DB_Host>"
az keyvault secret set --vault-name your-key-vault-name --name db-port --value "ModusETP_DB_Port"
az keyvault secret set --vault-name your-key-vault-name --name db-name --value "ModusETP_DB_Name"
az keyvault secret set --vault-name your-key-vault-name --name db-username --value "ModusETP_dbadmin"
az keyvault secret set --vault-name your-key-vault-name --name db-password --value "ModusETP_DB_PW"
